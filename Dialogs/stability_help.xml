<?xml version="1.0"?>
<!-- Version: 2016-07-13 -->
<PropertyList>
 <name>stability_help_dialog</name>
 <layout>vbox</layout>
 <default-padding>2</default-padding>
 <modal>false</modal>
 <width>480</width>
 <group>
  <layout>hbox</layout>
  <empty><stretch>true</stretch></empty>
  <text>
   <label>Help</label>
  </text>
  <empty><stretch>true</stretch></empty>
  <button>
   <legend/>
   <key>Esc</key>
   <pref-width>16</pref-width>
   <pref-height>16</pref-height>
   <border>2</border>
   <binding>
    <command>dialog-close</command>
   </binding>
  </button>
 </group>

 <hrule/>
 <hrule/>

 <group>
  <height>240</height>
  <text>
   <x>2</x>
   <y>220</y>
   <label>
   This dialog enables an adjustment of the stability characteristic to your
   special needs. For this purpose factors for some stability coefficients
   are introduced.

   This variables are NOT auto-saved.
   Handling-settings will be overwritten.

   </label>
  </text>
 </group>

 <hrule/>
 <hrule/>

 <group>
  <layout>hbox</layout>
  <empty><stretch>true</stretch></empty>
  <button>
   <legend>Close</legend>
   <binding>
    <command>dialog-close</command>
   </binding>
  </button>
  <empty><stretch>true</stretch></empty>
 </group>

</PropertyList>
