<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>

 <filter>
  <name>fbxz</name>
  <debug>false</debug>
  <type>gain</type>
  <gain>1</gain>
  <input>
   <expression>
    <sqrt>
     <sum>
      <pow>
       <property>fdm/jsbsim/forces/fbx-aero-lbs</property>
       <value>2</value>
      </pow>
      <pow>
       <property>fdm/jsbsim/forces/fbz-aero-lbs</property>
       <value>2</value>
      </pow>
     </sum>
    </sqrt>
   </expression>
  </input>
  <output>sim/model/Vector/fbxz-aero-lbs</output>
 </filter>

 <filter>
  <name>fbxyz</name>
  <debug>false</debug>
  <type>gain</type>
  <gain>1</gain>
  <input>
   <expression>
    <sqrt>
     <sum>
      <pow>
       <property>fdm/jsbsim/forces/fbx-aero-lbs</property>
       <value>2</value>
      </pow>
      <pow>
       <property>fdm/jsbsim/forces/fby-aero-lbs</property>
       <value>2</value>
      </pow>
      <pow>
       <property>fdm/jsbsim/forces/fbz-aero-lbs</property>
       <value>2</value>
      </pow>
     </sum>
    </sqrt>
   </expression>
  </input>
  <output>sim/model/Vector/fbxyz-aero-lbs</output>
 </filter>

</PropertyList>
